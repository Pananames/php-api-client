{
  "name": "pananames/php-api2",
  "description": "PHP client for Pananames APIv2",
  "authors": [
    {
      "name": "URLSolutions Inc.",
      "homepage": "https://pananames.com",
      "role": "Developer",
      "email": "dev@fozzy.com"
    }
  ],
  "type": "library",
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "^7.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^1.4",
    "squizlabs/php_codesniffer": "^3.6"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Pananames\\Api\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pananames\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "phpunit --no-coverage"
    ]
  },
  "scripts-descriptions": {
    "test" : "Runs PHPUnit test cases without coverage report"
  }
}